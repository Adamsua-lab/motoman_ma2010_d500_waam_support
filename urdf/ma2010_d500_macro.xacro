<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find motoman_ma2010_support)/urdf/ma2010_macro.xacro" />
  <xacro:include filename="$(find motoman_motopos_d500_support)/urdf/motopos_d500_macro.xacro" />
  <xacro:macro name="ma2010_d500" params="prefix">

    <xacro:motoman_ma2010 prefix="${prefix}ma2010_"/>

    <xacro:motoman_motopos_d500 prefix="${prefix}d500_"/>
  </xacro:macro>
</robot>
